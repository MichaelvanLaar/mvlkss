    "johannschopplich/kirby-hashed-assets": "^3.0",
    "isaactopo/xmlsitemap": "^0.2.2",
    "bnomei/kirby3-robots-txt": "^1.5",
    "distantnative/retour-for-kirby": "^4.4",
    "fabianmichael/kirby-markdown-field": "^2.1",
    "adspectus/date-extended": "^1.1",
    "sylvainjule/colorextractor": "^1.0",
    "hananils/kirby-colors": "^1.7",
    "flokosiol/focus": "^3.1",
    "sylvainjule/autofocus": "^1.0",
    "felixhaeberle/kirby3-webp": "^1.0"
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    },
    "optimize-autoloader": true
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 kirby/router.php"
    ]
  }
}
